    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okonet/reason-conf-assets"
  },
  "bugs": {
    "url": "https://github.com/okonet/reason-conf-assets/issues"
  },
  "husky": {
    "hooks": {
