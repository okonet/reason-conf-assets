{
  "name": "reason-conf-assets",
  "private": true,
  "description": "Reason Conf Print Assets Generator",
  "version": "0.1.0",
  "author": "Andrey Okonetchnikov <andrey@okonet.ru>",
  "dependencies": {
    "gatsby": "^2.1.30",
    "gatsby-image": "^2.0.32",
    "gatsby-plugin-manifest": "^2.0.23",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.0.27",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-source-graphql": "^2.0.14",
    "gatsby-transformer-sharp": "^2.1.16",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-figma-primitives": "^1.0.0",
    "react-helmet": "^5.2.0",
    "stack-styled": "^0.1.0",
